options {
	directory "/var/cache/bind";
	allow-query { 127.0.0.0/8; 103.192.174.0/24; };
	forwarders {
		8.8.8.8;
		1.1.1.1;
	};

	response-policy {
		zone "trustpositifkominfo"
		policy cname blok.jkbtel.com
		max-policy-ttl 30
		log no;
		zone "forcesafesearch"
		log no;
	}
	recursive-only yes
	qname-wait-recurse no
	break-dnssec yes
	nsip-wait-recurse no;

	dnssec-validation auto;

	listen-on-v6 { any; };
};

